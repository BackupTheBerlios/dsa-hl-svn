                    <Dim type="Width" ><ImageDim imageset="RastullahLook-Images" image="HeaderBarSortUp" dimension="Width" /></Dim>
                    <Dim type="Height" ><ImageDim imageset="RastullahLook-Images" image="HeaderBarSortUp" dimension="Height" /></Dim>
                </Area>
